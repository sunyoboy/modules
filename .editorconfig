# top-most Editorconfig file
root = true

# 对所有文件都生效
[*]
charset= utf-8
indent_style = space
indent_size = 4
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.java]
indent_size = 4

[*.{html,xml,js}]
indent_size = 4

[*.{css,scss,json,yml}]
indent_size = 2

[*.md]
trim_trailing_whitespace = false
